{
    "name": "emmet",
    "displayName": "emmet",
    "description": "Emmet support for VS Code",
    "version": "0.0.1",
    "publisher": "ramyaraoa",
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Other"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/ramya-rao-a/vscode-emmet.git"
    },
    "activationEvents": [
        "onLanguage:html",
        "onLanguage:xml",
        "onLanguage:jade",
        "onLanguage:css",
        "onLanguage:scss",
        "onLanguage:less",
        "onLanguage:javascriptreact",
        "onLanguage:typescriptreact"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "Go configuration",
            "properties": {
                "emmet.autocomplete": {
                    "type": "boolean",
                    "default": true,
                    "description": "Experimental: Shows emmet expansions as suggestions"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "@emmetio/expand-abbreviation": "^0.5.0",
        "@emmetio/extract-abbreviation": "^0.1.1"
    }
}